<div class="converter-container">
  <div class="converter-item converter-item">
    <div class="converter-item__header">
      <img src="<%= Routes.static_path(@socket, "/images/calculator.show/petrol.svg") %>"/>
      </object>
      <div class="converter-item__details">
        <div class="converter-item__details-row">
          <div class="converter-item__details-name">
            Petrol burning
          </div>
          <div class="converter-item__details-value">
            <%= to_int(@converter[:liters]) %> liters
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
